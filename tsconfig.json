{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "rootDir": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true ,
    "typeRoots": ["./node_modules/@types", "./types"]
  },
  "include": ["./**/*.ts", "controllers/auth.controller.ts ts"],
  "exclude": ["node_modules"],
  
}
